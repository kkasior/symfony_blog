<table class="table table-dark" style="margin-top: 10%">
    <thead>
    <tr>


        <td scope="col">Post title:</td>


    </tr>
    </thead>
    <tbody>
    {% for post in postList %}
        <tr>

            <td>{{ post.title }}</td>
            <td><a href="{{post.title}}/details">Details</a></td>
            <td><a href="{{post.title}}/edit" >Edit</a></td>
            <td><a href="{{post.title}}/delete" >Delete</a></td>
        </tr>
    {% endfor %}
    </tbody>
</table>